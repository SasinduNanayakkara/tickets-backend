{
    "version": 2,
    "builds": [{"src": "./src/index.ts", "use": "@vercel/node"}],
    "routes": [{"src": "./src/(.*)", "dest": "/src/index.js"}],
    "rewrites": [{"source": "/(.*)", "destination": "/api"}],
    "functions": {
        "api/**/*.ts": {
          "runtime": "nodejs18"
        }
      }
}